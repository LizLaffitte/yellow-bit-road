<%= form_for @user do |f| %>
    <div><%= f.label :username %><br />
    <%= f.text_field :username %></div>
    <br />
    <div><%= f.label :email %><br />
    <%= f.email_field :email %>
    </div>
    <br />
    <div><%= f.label "Password" %><br />
    <%= f.password_field :password %>
    </div><br />
    <%= f.submit "Sign Up" %>
<% end %>